pipeline {
    agent any

    stages {
        stage('Checkout code') {
            steps {
                git branch: 'master', url: 'https://github.com/khouloud472/Gitexemple.git'
            }
        }

        stage('Compile code') {
            steps {
                sh 'javac *.java'
            }
        }
        stage('Execute code') {
            steps {
                sh 'java HelloWorld'
            }
        }
    }
}
